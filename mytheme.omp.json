{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{ .Shell }} - {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "diamond",
          "foreground": "#EFDCF9",
          "background": "#4c1f5e",
          "trailing_diamond": "\ue0b4",
          "template": " \uf07b "
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "#EFDCF9",
          "background": "#170B3B",
          "template": "{{.Path }} ",
          "properties": {
            "folder_icon": "...",
            "folder_separator_icon": "/",
            "home_icon": "~",
            "max_depth": 1,
            "style": "agnoster_short"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "#EFDCF9",
          "background": "#341948",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "exit",
          "style": "diamond",
          "foreground": "#EFDCF9",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#FF3C3C{{ end }}"
          ],
          "background": "#69307A",
          "trailing_diamond": "\ue0b4",
          "template": " \uf7d4{{ if gt .Code 0 }} {{.Code}}{{ end }} ",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "type": "python",
          "style": "diamond",
          "foreground": "#E4F34A",
          "background": "#4c1f5e",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue235{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "go",
          "style": "diamond",
          "foreground": "#7FD5EA",
          "background": "#4c1f5e",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ufcd1{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "node",
          "style": "diamond",
          "foreground": "#42E66C",
          "background": "#4c1f5e",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "ruby",
          "style": "diamond",
          "foreground": "#E64747",
          "background": "#4c1f5e",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "java",
          "style": "diamond",
          "foreground": "#E64747",
          "background": "#4c1f5e",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#EFDCF9",
          "background": "#170B3B",
          "leading_diamond": " \ue0b6",
          "template": " {{ .UserName }}@{{ .HostName }}  "
        },
        {
          "type": "time",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "#EFDCF9",
          "background": "#4c1f5e",
          "template": " {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "15:04"
          }
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "#EFDCF9",
          "background": "#341948",
          "template": " {{ .FormattedMs }} ",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "diamond",
          "foreground": "#EFDCF9",
          "background": "#4c1f5e",
          "trailing_diamond": "\ue0b4",
          "template": " \uf120 "
        }
      ],
      "newline": true
    }
  ]
}
